CREATE COMPUTE MODULE MOVE_LOCAL_ENV
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		SET OutputRoot = InputRoot;

		SET OutputLocalEnvironment = InputLocalEnvironment;
		SET OutputRoot.SOAP.localEnv = InputLocalEnvironment;

		RETURN TRUE;
	END;
END MODULE;

CREATE COMPUTE MODULE RESTORE_LOCAL_ENV
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		SET OutputRoot = InputRoot;

		DELETE FIELD OutputRoot.SOAP.localEnv;

		RETURN TRUE;
	END;
END MODULE;