CREATE COMPUTE MODULE CLEAR_LENV_FAULT
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		DECLARE soapenv NAMESPACE 'http://schemas.xmlsoap.org/soap/envelope/';
		SET OutputRoot = InputRoot;

		DELETE FIELD OutputRoot.XMLNSC.soapenv:Envelope.soapenv:Body.soapenv:localEnv;

		RETURN TRUE;
	END;
END MODULE;